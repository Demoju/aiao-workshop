<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tableorder.customer.mapper.CategoryMapper">
    
    <select id="selectCategories" resultType="com.tableorder.domain.Category">
        SELECT category_id, category_name, store_id, created_at
        FROM categories
        ORDER BY category_id
    </select>
    
</mapper>
